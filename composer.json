	"license": "BSD 2-Clause",
    "authors": [
        {
            "name": "Daniel Stainback",
            "email": "daniel@lyften.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "illuminate/support": "~5.1",
        "illuminate/cache": "~5.1"
    },
    "suggest": {
        "illuminate/database": "Allows for storing of currencies in the database"
    },
    "autoload": {
        "files": [
            "src/Torann/Currency/helpers.php"
        ],
        "psr-4": {
            "Torann\\Currency\\": "src/Torann/Currency"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.2-dev"
        }
    }
